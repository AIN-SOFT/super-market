<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.soft1841.sm.controller.GuanLiController">
    <FlowPane fx:id="guanliPane" hgap="100" vgap="600"
          AnchorPane.topAnchor="80"
          AnchorPane.rightAnchor="10"
          AnchorPane.leftAnchor="140"/>
    <Button text="新增管理员" AnchorPane.topAnchor="20"
            AnchorPane.leftAnchor="10" onAction="#addGuanLi" styleClass="button-blue" />
    <StackPane styleClass="box" AnchorPane.topAnchor="200"  >
        <ImageView fx:id="guanliImg" fitWidth="600" fitHeight="300">
        </ImageView>
    </StackPane>

</AnchorPane>
